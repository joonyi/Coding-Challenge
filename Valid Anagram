class Solution {
public:
    bool isAnagram(string s, string t) {
        vector<int> alphabert(26,0);
        for(int i=0; i<s.size(); i++){
            alphabert[s[i]-'a']+=1;
        }
        for(int i=0; i<t.size(); i++){
            alphabert[t[i]-'a']-=1;
        }
        for(int i=0;i<alphabert.size();i++){
            if(alphabert[i]!=0) return false;
        }
        return true;
    }
};
