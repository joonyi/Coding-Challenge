class Solution {
public:
    bool isIsomorphic(string s, string t) {
        map<char, int> char1, char2;
        
        for(int i=0; i<s.size(); i++){
            if(char1[s[i]]==0 && char2[t[i]]==0)
                char1[s[i]]=char2[t[i]]=i+1;
                
            else if (char1[s[i]]!=char2[t[i]]){
                return false;
            }
        }
        return true;
        
        
        
    }
};
